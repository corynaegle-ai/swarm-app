{
  "annotations": { "list": [] },
  "editable": true,
  "id": null,
  "title": "Swarm Agent Performance",
  "uid": "swarm-agent-performance",
  "version": 1,
  "refresh": "30s",
  "schemaVersion": 39,
  "tags": ["swarm", "agents"],
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Execution Success Rate",
      "type": "gauge",
      "gridPos": { "h": 5, "w": 6, "x": 0, "y": 0 },
      "targets": [{ "expr": "sum(swarm_agent_executions_total{status=\"success\"}) / sum(swarm_agent_executions_total) * 100", "refId": "A" }],
      "fieldConfig": {
        "defaults": {
          "min": 0, "max": 100, "unit": "percent",
          "thresholds": { "steps": [{ "color": "red", "value": null }, { "color": "yellow", "value": 90 }, { "color": "green", "value": 95 }] }
        }
      }
    },
    {
      "id": 2,
      "title": "Avg Ticket Duration",
      "type": "stat",
      "gridPos": { "h": 5, "w": 6, "x": 6, "y": 0 },
      "targets": [{ "expr": "avg(rate(swarm_ticket_duration_seconds_sum[5m]) / rate(swarm_ticket_duration_seconds_count[5m]))", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "s", "color": { "mode": "thresholds" }, "thresholds": { "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 60 }, { "color": "red", "value": 300 }] } } }
    },
    {
      "id": 3,
      "title": "Total Executions (24h)",
      "type": "stat",
      "gridPos": { "h": 5, "w": 6, "x": 12, "y": 0 },
      "targets": [{ "expr": "sum(increase(swarm_agent_executions_total[24h]))", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "short" } }
    },
    {
      "id": 4,
      "title": "Claude API Calls (1h)",
      "type": "stat",
      "gridPos": { "h": 5, "w": 6, "x": 18, "y": 0 },
      "targets": [{ "expr": "sum(increase(swarm_claude_api_calls_total[1h]))", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "short" } }
    },
    {
      "id": 5,
      "title": "Executions by Agent Type",
      "type": "barchart",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 5 },
      "targets": [{ "expr": "sum by (agent_type) (increase(swarm_agent_executions_total[24h]))", "legendFormat": "{{agent_type}}", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "short" } }
    },
    {
      "id": 6,
      "title": "Executions by Status",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 5 },
      "targets": [{ "expr": "sum by (status) (swarm_agent_executions_total)", "legendFormat": "{{status}}", "refId": "A" }],
      "options": { "legend": { "displayMode": "table", "placement": "right" } }
    },
    {
      "id": 7,
      "title": "Claude Models Used",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 5 },
      "targets": [{ "expr": "sum by (model) (swarm_claude_api_calls_total)", "legendFormat": "{{model}}", "refId": "A" }],
      "options": { "legend": { "displayMode": "table", "placement": "right" } }
    },
    {
      "id": 8,
      "title": "Token Usage Over Time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 13 },
      "targets": [
        { "expr": "sum(rate(swarm_claude_api_tokens{direction=\"input\"}[5m]))", "legendFormat": "Input Tokens", "refId": "A" },
        { "expr": "sum(rate(swarm_claude_api_tokens{direction=\"output\"}[5m]))", "legendFormat": "Output Tokens", "refId": "B" }
      ],
      "fieldConfig": { "defaults": { "unit": "short" } }
    },
    {
      "id": 9,
      "title": "Cost by Model (Cumulative)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 13 },
      "targets": [{ "expr": "sum by (model) (swarm_claude_api_cost_usd)", "legendFormat": "{{model}}", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "currencyUSD" } }
    },
    {
      "id": 10,
      "title": "Ticket Duration by Complexity",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 21 },
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(swarm_ticket_duration_seconds_bucket{complexity=\"low\"}[5m])) by (le))", "legendFormat": "Low P95", "refId": "A" },
        { "expr": "histogram_quantile(0.95, sum(rate(swarm_ticket_duration_seconds_bucket{complexity=\"medium\"}[5m])) by (le))", "legendFormat": "Medium P95", "refId": "B" },
        { "expr": "histogram_quantile(0.95, sum(rate(swarm_ticket_duration_seconds_bucket{complexity=\"high\"}[5m])) by (le))", "legendFormat": "High P95", "refId": "C" }
      ],
      "fieldConfig": { "defaults": { "unit": "s" } }
    }
  ]
}
